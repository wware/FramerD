(use-module! fdmaint)
(unless (file-exists? "dbtest.pool")
  (make-file-pool "dbtest.pool" 4000 @17/0)
  (label-pool! "dbtest.pool" "dbtest.framerd.org"))
(unless (file-exists? "dbtest.index")
  (make-file-index "dbtest.index" 250000))
(use-localhost!)
(serve-pool "dbtest.pool")
(serve-index "dbtest.index")
